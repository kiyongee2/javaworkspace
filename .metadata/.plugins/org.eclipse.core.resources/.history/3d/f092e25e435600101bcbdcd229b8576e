package datatype;

public class NumberType {

	public static void main(String[] args) {
		/* 자료형의 범위
		   데이터의 용량 - 바이트(byte)
		   숫자 3, 영문자 - 1byte(8bit)
		   1bit - 0, 1
		*/ 
		int money = 2000000000;
		System.out.println(money);
		
		long money2 = 3000000000L; //'L' or 'l'
		System.out.println(money2);

	}

}
